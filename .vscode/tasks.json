    "version": "2.0.0",
    "tasks": [
        {
            "label": "clean",
            "type": "shell",
            "command": "mvn -B clean",
            "group": "build"
        },
        {
            "label": "package",
            "type": "shell",
            "command": "mvn -B package",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
